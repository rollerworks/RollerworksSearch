{
    "name": "rollerworks/search-symfony-validator",
    "type": "library",
    "description": "Symfony validator extension for RollerworksSearch",
    "keywords": [
        "rollerworks",
        "search",
        "validation",
        "symfony"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastiaan Stok",
            "email": "s.stok@rollercapes.net"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/Rollerworks/search-symfony-validator/contributors"
        }
    ],
    "require": {
        "php": ">=7.2",
        "rollerworks/search": "^2.0@dev,>=2.0.0-BETA1",
        "symfony/validator": "^4.4 || ^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5.4",
        "symfony/phpunit-bridge": "^4.4 || ^5.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Rollerworks\\Component\\Search\\Extension\\Symfony\\Validator\\": ""
        },
        "exclude-from-classmap": [
            "test/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Rollerworks\\Component\\Search\\Extension\\Symfony\\Validator\\Tests\\": "Tests/"
        }
    }
}
